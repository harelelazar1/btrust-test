<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Liveness Demo" time-out="7800000" >

    <listeners>
        <listener class-name="utilities.TestListener"/>
        <listener class-name="utilities.SuiteListener"/>
    </listeners>


    <test name="Liveness DEMO" parallel="methods" thread-count="2">
        <classes>
            <class name="selenium.ngliveness.test.passive.SanityTest"/>
        </classes>
    </test> <!-- Test -->

</suite> <!-- Suite -->