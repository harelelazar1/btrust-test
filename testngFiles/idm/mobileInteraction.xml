<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="apiTests">
    <listeners>
        <listener class-name="utilities.TestListener"/>
        <listener class-name="utilities.SuiteListener"/>
    </listeners>
    <test name="Btrust one platform - mobile interaction ui tests">
        <packages>
            <package name="btrust.onboardingProcess.ui.test.btrust1.mobileInteraction.*"/>
        </packages>
    </test> <!-- Test -->

    <test name="Btrust one platform - mobile interaction api tests - V1 endpoint">
        <packages>
            <package name="btrust.onboardingProcess.api.v1.*"/>
        </packages>
    </test>

    <test name="Btrust one platform - mobile interaction api tests - V2 endpoint">
        <packages>
            <package name="btrust.onboardingProcess.api.v2.*"/>
        </packages>
    </test>

    <test name="Btrust one platform - mobile interaction api tests - Harel endpoint">
        <packages>
            <package name="btrust.onboardingProcess.api.harel.*"/>
        </packages>
    </test>
</suite> <!-- Suite -->